generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:Rootoorn@localhost:3306/back"
  // url      = "mysql://app:apppass@db-dev:3306/back"
}

// prisma/schema.prisma

model Server {
  id            Int      @id @default(autoincrement())
  ip            String   @unique
  configured    Boolean  @default(false)
  range         Int
  ownerClientId Int      @default(9999999999999)
  password      String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}

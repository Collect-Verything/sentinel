generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Server {
  id            Int      @id @default(autoincrement())
  ip            String   @unique
  configured    Boolean  @default(false)
  range         Int
  ownerClientId Int
  password      String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
